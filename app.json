{
  "expo": {
    "name": "NFC Alarm 2025",
    "slug": "nfca-apr-2025",
    "version": "2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.briantan.nfcaalarm",
      "buildNumber": "2",
      "infoPlist": {
        "NFCReaderUsageDescription": "This app uses NFC to scan tags for alarm dismissal",
        "com.apple.developer.nfc.readersession.formats": [
          "NDEF"
        ],
        "NSUserNotificationUsageDescription": "Allow NFC Alarm 2025 to send notifications.",
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification"
        ],
        "NSMicrophoneUsageDescription": "This app does not require microphone access"
      },
      "entitlements": {
        "com.apple.developer.nfc.readersession.formats": [
          "NDEF"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "This app uses NFC to scan tags for alarm dismissal"
        }
      ],
      "expo-router",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/ringtone.mp3",
            "./assets/sounds/notiAlarm.mp3",
            "./assets/sounds/netflix.mp3"
          ]
        }
      ],
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "901b6058-a1ff-49fb-abb7-5b541cf22d37"
      }
    }
  }
}
